{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "server/api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/$1"
      }
    ]
  }
  